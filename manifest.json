{
    "name": "Gamma",
    "version": "2.1.3",
    "description": "Have a seamless AI Learning experience or generate stunning images, all without ever having to leave your website.",
    "default_locale": "en",
    "icons": {
        "16": "assets/icons/icon-16.png",
        "48": "assets/icons/icon-48.png",
        "128": "assets/icons/icon-128.png"
    },
    "host_permissions": [
        "<all_urls>"
    ],
    "permissions": [
        "storage",
        "activeTab",
        "tabs",
        "videoCapture"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup/popup.html"
    },
    "options_page": "options/options.html",
    "manifest_version": 3,
    "web_accessible_resources": [
        {
            "resources": ["styles/wellness_check.css"],
            "matches": ["<all_urls>"]
        }
    ]
}